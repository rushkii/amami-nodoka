{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "lf",
    "printWidth": 100,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
